{
    "extends": "./.nuxt/tsconfig.json",
    "compilerOptions": {
        "experimentalDecorators": true,
        "emitDecoratorMetadata": true,
        "strictPropertyInitialization": false,
        "target": "ES2020",
        "module": "ESNext",
        "moduleResolution": "Bundler",
        "skipLibCheck": true,
        "resolveJsonModule": true
    }
}